        motogp obj15 = new motogp("Bradley Smith", "Aprilia Racing Team Gresini", "England", "+39.682");
        
        System.out.println("Podium 1:\t"+obj1.name+"\t"+" Teammate :"+obj1.motor+"\t\t"+" Country :"+obj1.country+"\t"+" Last Score :"+obj1.score);
        System.out.println("Podium 2:\t"+obj2.name+"\t"+" Teammate :"+obj2.motor+"\t"+" Country :"+obj2.country+"\t\t"+" Last Score :"+obj2.score);
        System.out.println("Podium 3:\t"+obj3.name+"\t"+" Teammate :"+obj3.motor+"\t\t\t"+" Country :"+obj3.country+"\t\t"+" Last Score :"+obj3.score);
