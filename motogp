package abstract_inherit_poly_motogp;

public class motogp {
	String name, country, motor, score;
	
	motogp() {
        this.name = "Fabio Quartararo";
        this.motor = "Petronas Yamaha SRT";
        this.country = "France";
        this.score = "41'23.796";
    }

    motogp(String n,String m, String c, String s){
        this.name = n;
        this.motor = m;
        this.country = c;
        this.score = s;
    }

    public void printmotogp() {
        motogp obj1 = new motogp();
        motogp obj2 = new motogp("Maverick Vinales", "Monster Energy Yamaha", "Spain", "+4.603");
        motogp obj3 = new motogp("Andrea Dovizioso", "Ducati", "Italy", "+5.946");
