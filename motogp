        System.out.println("Podium 12:\t"+obj12.name+"\t\t"+" Teammate :"+obj12.motor+"\t\t"+" Country :"+obj12.country+"\t\t"+" Last Score :"+obj12.score);
        System.out.println("Podium 13:\t"+obj13.name+"\t\t"+" Teammate :"+obj13.motor+"\t"+" Country :"+obj13.country+"\t"+" Last Score :"+obj13.score);
        System.out.println("Podium 14:\t"+obj14.name+"\t\t"+" Teammate :"+obj14.motor+"\t"+" Country :"+obj14.country+"\t\t"+" Last Score :"+obj14.score);
