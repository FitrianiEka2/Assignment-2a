        System.out.println("Podium 5:\t"+obj5.name+"\t"+" Teammate :"+obj5.motor+"\t\t"+" Country :"+obj5.country+"\t\t"+" Last Score :"+obj5.score);
        System.out.println("Podium 6:\t"+obj6.name+"\t\t"+" Teammate :"+obj6.motor+"\t"+" Country :"+obj6.country+"\t\t"+" Last Score :"+obj6.score);
        System.out.println("Podium 7:\t"+obj7.name+"\t"+" Teammate :"+obj7.motor+"\t\t"+" Country :"+obj7.country+"\t\t"+" Last Score :"+obj7.score);
        System.out.println("Podium 8:\t"+obj2.name+"\t"+" Teammate :"+obj8.motor+"\t\t"+" Country :"+obj8.country+"\t"+" Last Score :"+obj8.score);
        System.out.println("Podium 9:\t"+obj9.name+"\t\t"+" Teammate :"+obj9.motor+"\t\t\t"+" Country :"+obj9.country+"\t\t"+" Last Score :"+obj9.score);
